{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "0.1.4",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "MIT",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "git+https://github.com/SophiaSaiada/gatsby-theme-minimal-blog.git",
    "@lekoarts/gatsby-theme-minimal-blog-core": "git+https://github.com/SophiaSaiada/gatsby-theme-minimal-blog-core.git",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.13.3",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-remark-katex": "^3.1.20",
    "gatsby-transformer-remark": "^2.6.45",
    "katex": "^0.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-with-direction": "^1.3.1",
    "remark-html-katex": "^2.0.0",
    "remark-math": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "gh-pages": "^2.1.1"
  }
}
